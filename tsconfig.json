{
  "compilerOptions": {
    "outDir": "./dist",
    "noImplicitAny": true,
    "module": "ESNext",
    "target": "ES2015",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "resolveJsonModule": true
  },
  "exclude": ["node_modules", "dist"]
}
